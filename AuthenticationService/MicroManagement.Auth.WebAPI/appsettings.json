{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "my_micro_management_auth_issuer",
    "Audience": "my_micro_management_auth_audience",
    "AccessTokenDurationInMinutes": "5",
    "RefreshTokenDurationInDays": "30"
  }
}
